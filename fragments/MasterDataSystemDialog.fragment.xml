<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>CreateOffer.MasterDataSystem}"
		search="handleSearch"
		confirm="onMasterDataSystemSelect"		
		items="{/}"
		cancel="onCancelPress"
		rememberSelections="true">
		<StandardListItem
			title="{System}"
			description="{Description}"
			selected="{Selected}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>